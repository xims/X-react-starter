{
  "name": "X-react-starter",
  "version": "1.1.1",
  "description": "Simple React.js starter example built with React.js stack",
  "author": "Maxim Shklyar, kisla interactive",
  "license": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/xims/X-react-starter"
  },
  "scripts": {
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "jest --coverage",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:webpack_dev": "cross-env NODE_ENV=development webpack --config webpack.config.prod.js",
    "bc": "npm run build && npm run build:copyall_win",
    "bu": "npm run build && npm run build:copyupd_win",
    "bv": "npm version patch",
    "build:copyall_win": "xcopy dist\\* ..\\WS /E /Y /D && xcopy static\\* ..\\WS /E /Y /D",
    "build:copyupd_win": "copy dist\\bundle.js ..\\WS\\ && copy dist\\style.css ..\\WS\\"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "jest-cli": "^12.0.2",
    "node-sass": "^3.5.3",
    "proxy-middleware": "^0.15.0",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "ampersand-app": "^1.0.4",
    "get-size": "^2.0.2",
    "jquery": "^2.2.3",
    "object-assign": "^4.0.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-ga": "^1.3.3",
    "react-imageloader": "^2.1.0",
    "react-motion": "^0.4.3",
    "react-router": "^2.3.0",
    "superagent": "^1.8.3",
    "validator": "^5.2.0",
    "x2js": "^2.0.1"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  }
}
